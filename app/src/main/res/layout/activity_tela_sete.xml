            android:fontFamily="serif"
            android:textSize="14sp"
            android:layout_width="272dp"
            android:layout_height="98dp" android:id="@+id/pro1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.586"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.496"/>
    <TextView
            android:text="Premium"
            android:textStyle="bold"
            android:textAlignment="center"
            android:fontFamily="serif"
            android:textSize="14sp"
            android:layout_width="298dp"
            android:layout_height="103dp" android:id="@+id/premium1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.89" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.495"/>
    <TextView
            android:text="\nMassagem relaxante Design de barba Corte de cabelo com atenção aos detalhes Marcação de horário preferencial\nR$ 100,00"
            android:textAlignment="center"
            android:fontFamily="serif"
            android:textSize="14sp"
            android:layout_width="298dp"
            android:layout_height="103dp" android:id="@+id/premium"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.89" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.495"/>
